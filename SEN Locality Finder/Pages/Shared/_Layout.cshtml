<!DOCTYPE html>
<html>
<head>
    <META http-equiv="Content-Type" content="text/html; charset= ISO-8859-1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SEN Locality Finder</title>

    <script src="https://code.jquery.com/jquery-3.3.1.slim.js" integrity="sha256-fNXJFIlca05BIO2Y5zh1xrShK3ME+/lYZ0j+ChxX2DA="
            crossorigin="anonymous"></script>
    <!-- Compiled and minified JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>

    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

    <link href="~/lib/tabulator/css/tabulator.min.css" rel="stylesheet">
    <script type="text/javascript" src="~/lib/tabulator/js/tabulator.min.js"></script>

    <script src="~/js/site.min.js"></script> <!-- minified JS for page load speed -->
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
</head>
<body>

    <nav class="navbar_content">
        <div class="nav-wrapper">
            <a asp-page="./Index"><img src="~/images/editedlogo.png" class="sen_logo" /></a>
            <a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="material-icons">menu</i></a>
            <ul class="right hide-on-med-and-down">
                <li><a asp-page="./Index"><i class="material-icons left">home</i>Home</a></li>
                <li><a asp-page="./About"><i class="material-icons left">info</i>About Us</a></li>
                @*<li><a asp-page="./Privacy"><i class="material-icons left">details</i>Privacy Policy</a></li>
                    <li><a asp-page="./Contact"><i class="material-icons left">contact_mail</i>Contact Us</a></li>*@
                <li><a asp-page="./Demographics"><i class="material-icons left">bar_chart</i>Demographics</a></li>
                <li><a asp-page="./Accessibility"><i class="material-icons left">accessibility_new</i>Accessibility</a></li>
                <li><a asp-page="./FAQ"><i class="material-icons left">question_answer</i>FAQ's</a></li>
            </ul>
        </div>
    </nav>

    <ul class="sidenav" id="mobile-demo">
        <li><a asp-page="./Index"><i class="material-icons left">home</i>Home</a></li>
        <li><a asp-page="./About"><i class="material-icons left">info</i>About Us</a></li>
        @*<li><a asp-page="./Privacy"><i class="material-icons left">details</i>Privacy Policy</a></li>*@
        @*<li><a asp-page="./Contact"><i class="material-icons left">contact_mail</i>Contact Us</a></li>*@
        <li><a asp-page="./Demographics"><i class="material-icons left">bar_chart</i>Demographics</a></li>
        <li><a asp-page="./Accessibility"><i class="material-icons left">accessibility_new</i>Accessibility</a></li>
        <li><a asp-page="./FAQ"><i class="material-icons left">question_answer</i>FAQ's</a></li>
    </ul>


    <script>
        $(document).ready(function () {
            $('.sidenav').sidenav();
        });
    </script>


    <div class="container">
        @RenderBody()
        <hr />
        <footer class="footer">
            <div class="footer-bottom">
                <p class="footer-links">

                    <a asp-page="./Contact">Contact Us</a>
                    .
                    <a asp-page="./Privacy">Privacy Policy</a>

                    |

                    Copyright &copy; SEN Locality Finder 2019
                </p>

            </div>
        </footer>
    </div>

    <environment exclude="Development">

    </environment>

    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-135169306-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-135169306-1');
    </script>

    @*@RenderSection("Scripts", required: false)*@
</body>
</html>
